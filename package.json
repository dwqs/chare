{
  "name": "chare",
  "version": "0.0.3",
  "description": "A simple CLI for scaffolding Vue.js projects.",
  "preferGlobal": true,
  "bin": {
    "chare": "bin/chare",
    "chare-init": "bin/chare-init",
    "chare-list": "bin/chare-list"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwqs/chare.git"
  },
  "keywords": [
    "chare",
    "cli",
    "spa",
    "scaffold",
    "front end scaffold"
  ],
  "author": "dwqs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dwqs/chare/issues"
  },
  "homepage": "https://github.com/dwqs/chare#readme",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run lint && npm run e2e",
    "lint": "eslint test/e2e/test.js lib bin/* --env mocha",
    "e2e": "rimraf test/e2e/mock-template-build/*.* && cross-env BABEL_ENV=development mocha test/e2e/test.js --slow 1000 --compilers js:babel-core/register"
  },
  "dependencies": {
    "async": "^2.1.4",
    "chalk": "^1.1.3",
    "consolidate": "^0.14.5",
    "download-git-repo": "^0.2.1",
    "handlebars": "^4.0.5",
    "inquirer": "^2.0.0",
    "metalsmith": "^2.3.0",
    "minimatch": "^3.0.0",
    "multimatch": "^2.1.0",
    "ora": "^0.4.0",
    "read-metadata": "^1.0.0",
    "request": "^2.79.0",
    "rimraf": "^2.5.4",
    "semver": "^5.3.0",
    "uid": "0.0.2",
    "validate-npm-package-name": "^2.2.2",
    "commander": "^2.9.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "eslint": "^2.7.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "mocha": "^2.4.5"
  },
  "engines": {
    "node": ">=5.0.0"
  }
}
